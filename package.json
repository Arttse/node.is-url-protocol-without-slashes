{
  "name": "is-url-protocol-without-slashes",
  "version": "1.0.3",
  "description": "Checks for the presence of URL protocol without a slashes (colon-slash-slash)",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arttse/node.is-url-protocol-without-slashes.git"
  },
  "keywords": [
    "is",
    "url",
    "uri",
    "protocol",
    "without",
    "slashes",
    "colon",
    "slash",
    "requires",
    "check",
    "presence"
  ],
  "author": "Nikita «Arttse» Bystrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arttse/node.is-url-protocol-without-slashes/issues"
  },
  "homepage": "https://github.com/Arttse/node.is-url-protocol-without-slashes#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": false,
    "rules": {
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "Property": true
          }
        }
      ],
      "no-spaced-func": 0,
      "space-in-parens": [
        "error",
        "always"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "padded-blocks": 0,
      "indent": [
        "error",
        2
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": true,
          "afterColon": true,
          "align": "colon"
        }
      ],
      "no-extra-parens": [
        "warn"
      ]
    }
  }
}
