{
  "name": "is-url-protocol-without-slashes",
  "version": "0.0.0",
  "description": "Checks for the presence of URL protocol without a slashes",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "engines" : {
    "node" : ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arttse/node.is-url-protocol-without-slashes.git"
  },
  "keywords": [
    "is",
    "url",
    "protocol",
    "without",
    "slashes"
  ],
  "author": "Nikita «Arttse» Bystrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arttse/node.is-url-protocol-without-slashes/issues"
  },
  "homepage": "https://github.com/Arttse/node.is-url-protocol-without-slashes#readme",
  "devDependencies": {
    "ava": "*",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "ImportDeclaration": true,
            "VariableDeclarator": true,
            "Property": true
          }
        }
      ],
      "no-spaced-func": 0,
      "space-in-parens": [
        "error",
        "always"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "padded-blocks": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        2
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": true,
          "afterColon": true,
          "align": "colon"
        }
      ]
    }
  }
}
